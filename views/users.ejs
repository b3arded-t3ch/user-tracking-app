<h1>User List</h1>
<table>
    <thead>
        <tr>
            <th>Username</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Organization</th>
            <th>Country</th>
            <th>Permission Level</th>
            <th>Registration Date</th>
            <th>Last Login Date</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.organization %></td>
                <td><%= user.country %></td>
                <td><%= user.permissionLevel %></td>
                <td><%= user.registrationDate.toDateString() %></td>
                <td><%= user.lastLoginDate ? user.lastLoginDate.toDateString() : 'N/A' %></td>
                <td><a href="/users/<%= user._id %>">View Details</a></td>
            </tr>
        <% }) %>
    </tbody>
</table>
<a href="/logout">Logout</a>
